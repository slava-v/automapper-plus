    "autoload-dev": {
        "psr-4": {
            "spec\\AutoMapperPlus\\":  "spec/"
        }
    },
    "require": {
        "lstrojny/functional-php": "^1.6",
        "php": ">=7.1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "symfony/debug": "^3.3",
        "phpunit/phpunit": "^6.4",
        "phpspec/phpspec": "^4.2"
    }
}
