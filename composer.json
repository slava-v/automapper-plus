{
    "name": "mark-gerarts/auto-mapper-plus",
    "description": "An AutoMapper for PHP",
    "license": "MIT",
    "type": "library",
    "keywords": ["automapper", "converter", "object-to-object"],
    "authors": [
        {
            "name": "Mark Gerarts",
            "email": "mark.gerarts@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AutoMapperPlus\\":  ["src/"]
        }
